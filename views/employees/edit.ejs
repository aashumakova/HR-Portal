<%- include('../partials/header') %>

<form action="/employees/<%= employee.id %>?_method=PUT" method="POST" autocomplete="off">
    <input type="text" name="name" value="<%= employee.name %>">
    <input type="checkbox" name="license" value="<%= employee.license ? 'Yes' : 'No' %>">License Up To Date</input>
    <button type="submit">Update Employee Information</button>
</form>

<%- include('../partials/footer') %>